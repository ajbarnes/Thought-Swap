{
  "extends": [
    "plugin:prettier/recommended",
    "plugin:react/recommended",
    "plugin:import/recommended"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "parserOptions": {
    "ecmaVersion": 9
  },
  "rules": {
    "prettier/prettier": ["error", {
        "singleQuote": true,
        "semi": false
      }]
  }
}
